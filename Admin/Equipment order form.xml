<?xml version="1.0" encoding="UTF-8"?>
<QAConfig>
  <head>
    <title>Equipment order form</title>
    </head>
  <savefilename>Equipment order form</savefilename>
  <type>Order</type>
  <form>


 
    <section>
      <name freq="start" colour="#F0E68C">Equipment order form</name>
      <fields>
        <field required="true"  title="Inspector Name" type="varchar" systemvalue="logon">
        </field>
        <field   required="true" title="Staff Number" type="int">
        </field>
        <field  required="true" title="Date" type="datetime">
        </field>
        <field   title="No Stock Forms" type="checkbox">
        </field>
        <field   title="Disinfectant" type="checkbox">
        </field>
        <field   title="Feed Sample Bags" type="checkbox">
        </field>
        <field  required="false" title="Water Proof Jacket (Size)" type="list" options="Small,Medium,Large,X-large">
        </field>
        <field  required="false" title="Water Proof Trousers (Size)" type="list" options="Small,Medium,Large,X-large">
        </field>
        <field required="false" linked="false"  title="Wellingtons (Size)" type="list" options="5,6,7,8,9,10,11,12">
        </field>
        <field   title="Other" type="checkbox">
        </field>
        <field   title="Other details" type="varchar" hide="Other!=true" disabled="true">
        </field>
        
        <field  required="true" title="Method of collection" type="radio">
          <options>
            <option >Please Post</option>
            <option>Will collect from HQ</option>            
          </options>          
        </field>

     
       </fields>
      </section>

   

  </form>
</QAConfig>